{
    "version": 2,
    "functions": {
      "api/app.py": {
        "maxDuration": 60
      }
    },
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ]
    
  }
  